<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<!--这边的包路径和sql语句需要根据自身的表的写-->
<mapper namespace="com.example.auto_warehouse.mapper.RepositoryMapper">
    <select id = "getCellAttr" resultType="com.example.auto_warehouse.bean.Cell">
        select ch,cw,cd,maxWeight from auto_warehouseDB.cell where crid = #{crid} and cetype = #{cetype}
    </select>

</mapper>